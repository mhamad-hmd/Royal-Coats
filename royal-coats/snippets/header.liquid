<div id="headerContainer" class="headerContainer  /  flex-col  w-full ">
    {%- render 'announcement' -%}

    <div id="headerWrapper"
        class="onBlur flex items-center justify-between headerWrapper h-20 md:px-5 xs:px-0 headerFont  w-full">

        <div class="mobileNavContainer flex-1 md:hidden xs:block headerTrans">
            <div class="sideNav ml-4">
                <svg id="navBtn" class="navBtn" width="27" height="16" viewBox="0 0 27 16" fill="none"
                    xmlns="http://www.w3.org/2000/svg">
                    <path d="M0 2H26.7248" stroke="#815B17" stroke-width="2.17391" />
                    <line y1="7.91304" x2="26.7248" y2="7.91304" stroke="#815B17" stroke-width="2.17391" />
                    <line y1="14.913" x2="26.7248" y2="14.913" stroke="#815B17" stroke-width="2.17391" />
                </svg>

            </div>
        </div>

        <div class="firstHeader headerTrans / flex justify-start items-center /  ">
            <div class="w-fit relative  ">
                <h1 id="titleHeader" class="break-words titleHeader uppercase  tracking-widest">
                    {{shop.name}}
                </h1>
                <span id="swordSpan" class="swordSvg">
                    <svg id="swordSvg" width="14" height="69" viewBox="0 0 14 69" fill="none"
                        xmlns="http://www.w3.org/2000/svg">
                        <path id="swordPath"
                            d="M7.41856 3.20077e-05C2.90107 17.5126 0.596833 34.5938 0.30479 52.759C0.874946 49.2244 2.17585 45.948 4.24349 42.8251C3.01179 34.9914 4.04907 27.516 7.36444 21.7636C10.4814 29.6533 11.2838 37.8178 9.8337 44.8009C11.769 48.9778 12.6889 52.8159 13.0453 56.4705C13.2423 38.3116 12.2167 19.796 7.41856 3.20077e-05ZM4.34309 49.1061L4.36661 49.1392C5.8317 55.7482 5.33265 61.0938 4.1395 66.0175C5.7295 68.983 7.49808 69.2862 8.94683 67.604C7.56542 61.1025 7.75965 55.7002 9.174 50.7259C7.83681 48.8445 5.91803 47.9867 4.34309 49.1061Z" />
                    </svg>
                </span>
            </div>
        </div>
        <div class="secondHeader / flex justify-between items-center md:flex xs:hidden / mx-5 w-full ">

            <div class="leftMenus gap-5 flex justify-evenly items-center ">
                {% for link in linklists.main-menu.links %}
                {%- assign itemList = forloop.length / 2-%}
                {%- if forloop.index <= 2 -%} <div class="relative itemListHeaderContainer">
                    {%- if link.links != blank -%}

                    <a id="itemListHeader{{forloop.index}}"
                        class="leftItem itemListHeaderWrapper headerItems  cursor-pointer headerTrans">
                        <span class=" itemListHeader itemListHeaderLinks itemListHeaderParent">{{ link.title }}</span>
                    </a>

                    <div id="headerSubMenuContainer{{forloop.index}}" class="headerSubMenuContainer ">
                        <div id="headerSubMenuWrapper{{forloop.index}}" class="headerSubMenuWrapper ">
                            {%- for childLink in link.links -%}
                            <a class=" subItem " href="{{ childLink.url }}">{{ childLink.title }}</a>
                            {%- endfor -%}
                        </div>
                    </div>
                    {%- else -%}
                    <a id="itemListHeader{{forloop.index}}"
                        class="leftItem itemListHeaderWrapper headerTrans headerItems" href="{{ link.url }}">
                        <span class="itemListHeader">{{ link.title }}</span>
                    </a>
                    {%- endif -%}
            </div>
            {%- endif -%}

            {% endfor %}

        </div>
        <div class="rightMenus gap-5  flex justify-evenly items-center ">
            {% for link in linklists.main-menu.links %}
            {%- assign itemList = forloop.length / 2-%}
            {%- if forloop.index > 2 -%} <div class="relative itemListHeaderContainer">
                {%- if link.links != blank -%}

                <a id="itemListHeader{{forloop.index}}" class="leftItem itemListHeaderWrapper headerTrans headerItems"
                    href="{{ link.url }}">
                    <span class="itemListHeader itemListHeaderParent itemListHeaderLinks">{{ link.title }}</span>
                </a>

                <div id="headerSubMenuContainer{{forloop.index}}" class="headerSubMenuContainer ">
                    <div id="headerSubMenuWrapper{{forloop.index}}" class="headerSubMenuWrapper ">
                        {%- for childLink in link.links -%}
                        <a class=" subItem " href="{{ childLink.url }}">{{ childLink.title }}</a>
                        {%- endfor -%}
                    </div>
                </div>
                {%- else -%}
                <a id="itemListHeader{{forloop.index}}" class="leftItem headerTrans itemListHeaderWrapper headerItems"
                    href="{{ link.url }}">
                    <span class="itemListHeader ">{{ link.title }}</span>
                </a>
                {%- endif -%}
            </div>
            {%- endif -%}

            {% endfor %}
        </div>

    </div>

    <div class="thirdHeader / flex justify-end items-center xs:flex-1 md:flex-none /">

        <ul id="" class="">
            <div class="flex ">
                <li class="items headerItems headerTrans ">
                    <a id="cart" href="/cart">
                        <svg class="cartSvg" width="25" height="25" viewBox="0 0 32 32" fill="none"
                            xmlns="http://www.w3.org/2000/svg">
                            <path class="cartIconPath"
