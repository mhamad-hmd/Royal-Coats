                fill="#815B17" />
        </svg>

    </div>
    <div class="SideNavLinksWrapper relative ">
        {% for link in linklists.main-menu.links %}
        {%- if link.links != blank -%}
        <div id="itemListParentContainer{{forloop.index}}" class="itemListParentContainer flex flex-col">

            <a id="sideNavLinks{{forloop.index}}"
                class=" sideNavLinks sideNavMainElement / flex justify-between items-center / w-full cursor-pointer">
                {{link.title }}
